<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Ethernet -->
    <!-- Ethernet IP settings. -->
    <!-- Note that adding/removing/moving the items will need ethernet settings code change. -->
    <string-array name="ethernet_ip_settings">
        <!-- Use DHCP (Dynamic Host Configuration Protocol) for obtaining IP settings [CHAR LIMIT=25] -->
        <item>DHCP</item>
        <!-- Use statically defined IP settings [CHAR LIMIT=25]-->
        <item>Static</item>
    </string-array>

    <!-- Ethernet proxy settings. -->
    <!-- Note that adding/removing/moving the items will need ethernet settings code change. -->
    <string-array name="ethernet_proxy_settings">
        <!-- No HTTP proxy is used for the current ethernet network [CHAR LIMIT=25] -->
        <item>None</item>
        <!-- Manual HTTP proxy settings are used for the current ethernet network [CHAR LIMIT=25] -->
        <item>Manual</item>
    </string-array>
</resources>
